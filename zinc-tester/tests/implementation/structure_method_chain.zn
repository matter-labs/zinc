struct Data {
    a: field,
}

impl Data {
    pub fn new(a: field) -> Self {
        Self {
            a: a,
        }
    }

    pub fn double(self) -> Self {
        Self { a: self.a * 2 as field }
    }

    pub fn triple(self) -> Self {
        Self { a: self.a * 3 as field }
    }

    pub fn quadruple(self) -> Self {
        Self { a: self.a * 4 as field }
    }
}

fn main(witness: field) -> field {
    Data::new(witness)
        .double()
        .triple()
        .quadruple()
        .a
}
